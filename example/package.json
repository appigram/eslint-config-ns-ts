{
  "name": "example-for-eslint-config-ns-ts",
  "private": true,
  "version": "0.0.1",
  "description": "Example code for eslint-config-ns-ts",
  "scripts": {
    "coverage": "jest --coverage",
    "lint": "eslint --cache 'src/**/*.{js,jsx,ts,tsx}'",
    "lint-staged-js": "git diff --diff-filter=d --cached --name-only -z -- '*.ts' '*.tsx' '*.js' '*.jsx' | xargs -0 -I % sh -c 'git show \":%\" | eslint --cache --stdin --stdin-filename \"%\";'",
    "start": "webpack-dev-server --progress --colors",
    "test": "jest ./src",
    "watch-test": "jest --watch"
  },
  "author": "Stefan Natter",
  "license": "MIT",
  "lint-staged": {
    "*.js": [
      "npm run lint-staged-js",
      "git update-index --again",
      "jest --findRelatedTests"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^25.1.2",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "babel-loader": "^8.0.6",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.4",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-ns-ts": "file:..",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^23.7.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^2.3.0",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "jest-enzyme": "^7.1.2",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "ts-jest": "^25.2.0",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.5",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  }
}
